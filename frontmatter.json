{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.content.pageFolders": [
    {
      "title": "articles",
      "path": "[[workspace]]/data/articles"
    }
  ],
  "frontMatter.taxonomy.categories": [
    "boilerplates",
    "css",
    "docker",
    "javascript",
    "ui-component"
  ],
  "frontMatter.taxonomy.tags": [
    "api",
    "boilerplate",
    "cms",
    "hasura",
    "next.js",
    "react.js",
    "strapi",
    "tailwind"
  ],
  "frontMatter.preview.host": "http://localhost:3000/articles",
  "frontMatter.framework.startCommand": "yarn dev",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "boolean"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "keywords",
          "name": "keywords",
          "type": "choice",
          "choices": ["Next.js", "Prisma", "GraphQL", "CLI", "Data fetching"]
        },
        {
          "title": "lastmod",
          "name": "lastmod",
          "type": "datetime"
        }
      ]
    }
  ]
}
